<Project Sdk="Microsoft.NET.Sdk">
  
  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;netstandard2.1;net6.0;net45</TargetFrameworks>
    <Platforms>AnyCPU;x64;x86</Platforms>
    <Authors>EasyMicroservices</Authors>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>0.0.0.2</Version>
    <Description>Manage your files with a wrapper in everywhere.</Description>
    <Copyright>EasyMicroservice@gmail.com</Copyright>
    <PackageTags>file,filemanager,disk,memory</PackageTags>
    <PackageProjectUrl>https://github.com/Easy-Microservise/FileManager</PackageProjectUrl>
    <LangVersion>latest</LangVersion>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <DocumentationFile>.\bin\$(Configuration)\$(TargetFramework)\EasyMicroservices.FileManager.xml</DocumentationFile>
  </PropertyGroup>
  
</Project>
